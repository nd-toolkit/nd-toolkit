{
  "name": "ndk-source-code",
  "version": "0.0.90",
  "description": "[REPO ONLY]",
  "repository": "github:nd-toolkit/source-code",
  "license": "Unlicense",
  "main": "node_modules/ndk-source-builder/builder.js",
  "files": [
    "source"
  ],
  "scripts": {
    "build": "node .",
    "test": "node . test",
    "coverage": "node . coverage",
    "watch": "node . watch",
    "filtre-skip-ci": "node . filtre-skip-ci"
  },
  "dependencies": {
    "ndk-source-builder": "github:nd-toolkit/source-builder"
  },
  "eslintIgnore": [
    "source/node_modules"
  ],
  "nyc": {
    "exclude": [
      "!**/source/node_modules/"
    ],
    "reporter": [
      "lcovonly"
    ],
    "temp-dir": "./build/nyc_output",
    "report-dir": "./build/coverage"
  }
}