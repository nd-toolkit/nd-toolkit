{
  "name": "@nd-toolkit/source-code",
  "version": "0.0.0",
  "repository": "github:nd-toolkit/source-code",
  "license": "Unlicense",
  "main": "node_modules/@nd-toolkit/source-builder/builder.js",
  "scripts": {
    "postinstall": "node node_modules/@nd-toolkit/source-builder/bin/postinstall-source-code.js",
    "build": "node .",
    "test": "node . test",
    "coverage": "node . coverage",
    "watch": "node . watch",
    "filtre-skip-ci": "node . filtre-skip-ci"
  },
  "dependencies": {
    "@nd-toolkit/source-builder": "github:nd-toolkit/source-builder",
    "@nd-toolkit/ndk-project": "github:nd-toolkit/ndk-project"
  },
  "nyc": {
    "exclude": [
      "!**/node_modules/@ndk/"
    ],
    "reporter": [
      "lcovonly"
    ],
    "temp-dir": "./build/nyc_output",
    "report-dir": "./build/coverage"
  }
}